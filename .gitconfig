[user]
	name = Fabiano Frizzo
	email = ffrizzo@gmail.com
[credential]
	helper = osxkeychain
[color]
	diff = auto
  status = auto
  branch = auto
  interactive = auto
[merge]
	summary = true
[core]
	excludesfile = $HOME/.gitignore_global
	editor = open -a Atom
[push]
	default = current
[alias]
	st=status
	co=checkout
	br=branch
	rb=rebase
	mg=merge
	cm=commit -a --amend -C HEAD
	ci=commit -a -v -m
	pu=push
	pl=pull
